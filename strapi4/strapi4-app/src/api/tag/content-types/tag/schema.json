{
  "kind": "collectionType",
  "collectionName": "tags",
  "info": {
    "singularName": "tag",
    "pluralName": "tags",
    "displayName": "Tag"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "string",
      "regex": "^[a-z|0-9|-]+$",
      "required": true,
      "unique": true
    },
    "articles": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::news-article.news-article",
      "inversedBy": "tags"
    }
  }
}
