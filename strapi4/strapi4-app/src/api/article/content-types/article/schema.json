{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "string",
      "regex": "^[a-z0-9]+(-[a-z0-9]+)*$",
      "required": true,
      "unique": true
    },
    "date": {
      "type": "date"
    },
    "featureImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "content": {
      "type": "customField",
      "options": {
        "output": "Markdown",
        "preset": "standard"
      },
      "customField": "plugin::ckeditor.CKEditor",
      "required": true
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "mappedBy": "articles"
    }
  }
}
